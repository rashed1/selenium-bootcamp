<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Cigna Health Insurance" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="userName" value="yourusername"/>
    <parameter name="key" value="youraccesskey"/>
    <parameter name="os" value="windows"/>
    <parameter name="browser" value="firefox"/>
    <parameter name="browserVersion" value="43"/>
    <parameter name="url" value="http://www.cigna.com/"/>

    <test name = "Cigna home page">
        <classes>
            <class name="HomePage.HomeSearch"/>
        </classes>
    </test>


</suite>